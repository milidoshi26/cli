ECHO OFF
FOR /F "tokens=* USEBACKQ" %%F IN (`dotnet --version`) DO (
    SET dotnetversion=%%F
)
ECHO %dotnetversion%
SET /A old = 1
SET /A new = 6
if %dotnetversion% GEQ %old% (
    if %dotnetversion% LEQ %new% (
        echo ".NET version check passed"
    ) else (
    echo "Please install .NET Core 1.0.1 - .NET 5.x to use CredScan"
    exit 1
    )
)